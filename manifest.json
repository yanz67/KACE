{
  "name": "KACEExt",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Auto Set fields in KACE",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
	"default_title": "That's the tool tip",
	"default_popup": "popup.html"
  }
}